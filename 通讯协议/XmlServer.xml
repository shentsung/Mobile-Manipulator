			<ELEMENT Tag="Robot/Data/LastPos/@B"/>
			<ELEMENT Tag="Robot/Data/LastPos/@C"/>
			<ELEMENT Tag="Robot/Data/ActPos/@X"/>
			<ELEMENT Tag="Robot/Status"/>
			<ELEMENT Tag="Robot/Mode"/>
			<ELEMENT Tag="Robot/RobotLamp/GrenLamp/LightOn"/>			
		</XML>
	</SEND>
</ETHERNETKRL>
